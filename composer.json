{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "alexpechkarev/google-maps": "1.0.9",
        "awobaz/compoships": "^2.0",
        "bacon/bacon-qr-code": "^2.0",
        "barryvdh/laravel-dompdf": "^0.8.6",
        "cloutier/php-ipfs-api": "^0.0.6",
        "dimsav/laravel-translatable": "8.*",
        "facebook/graph-sdk": "^5.7",
        "geoip2/geoip2": "~2.0",
        "google/apiclient": "~2.0",
        "graham-campbell/throttle": "^7.5",
        "intervention/image": "^2.4",
        "laravel/framework": "5.5.*",
        "laravel/passport": "~4.0",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.2.0",
        "laravelcollective/remote": "5.5",
        "lcobucci/jwt": "3.3.3",
        "maatwebsite/excel": "~3.1.0",
        "mockery/mockery": "0.9.*",
        "phpseclib/phpseclib": "~2.0.6",
        "piwik/device-detector": "^3.11",
        "pragmarx/google2fa-laravel": "^2.0",
        "pragmarx/google2fa-qrcode": "^3.0",
        "propaganistas/laravel-phone": "^3.0",
        "rosell-dk/webp-convert": "^2.3",
        "sendgrid/sendgrid": "^7.3",
        "torann/geoip": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Http/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
